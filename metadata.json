{
  "name": "gozerhp-static",
  "version": "0.0.1",
  "author": "Gozer team",
  "summary": "Puppet module for declaring a static host",
  "license": "Apache License 2.0",
  "source": "https://www.github.com/gozerhp/puppet-static.git",
  "requirements": [
    { "name": "pe","version_requirement": "3.x" },
    { "name": "puppet","version_requirement": "3.x" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7"]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": ["20"]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["6.5","7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04","14.04"]
    }
  ],
  "description": "Generates a static server and defines virtual hosts.",
  "dependencies": [
    {
      "name": "puppetlabs/puppetlabs-apache"
    }
  ]
}
